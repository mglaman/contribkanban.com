{
    "name": "mglaman/contribkanban.com",
    "description": "Contribkanban.com",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Matt Glaman <nmd.matt@gmail.com>",
            "role": "Author"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.1",
        "amazeeio/drupal_integrations": "^0.4",
        "composer/installers": "^1.2 | ^2.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^3.0",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity": "~1.0",
        "drupal/gin": "^3.0@RC",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/jsonapi_hypermedia": "^1.6",
        "drupal/jsonapi_search_api": "^1.0@beta",
        "drupal/jsonapi_user_resources": "^1.0@alpha",
        "drupal/lagoon_logs": "^2.0 || ^3.0",
        "drupal/redis": "^1.1",
        "drupal/search_api": "^1.15",
        "drupal/simple_oauth": "^5.2.0",
        "drupal/swiftmailer": "^2.4",
        "drupal/upgrade_status": "^4.0",
        "drush/drush": "^12.5.2",
        "laminas/laminas-feed": "^2.22"
    },
    "require-dev": {
        "drupal/core-dev": "^10.1",
        "palantirnet/drupal-rector": "^0.20.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "excludes": [
                "sites/default/default.services.yml",
                "sites/default/default.settings.php"
            ],
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false,
                "[project-root]/.editorconfig": false
            },
            "allowed-packages": [
                "amazeeio/drupal_integrations"
            ]
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
