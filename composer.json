{
    "name": "mglaman/contribkanban.com",
    "description": "Contribkanban.com",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Matt Glaman <nmd.matt@gmail.com>",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/installers": "^1.2",
        "consolidation/robo": "^1.1",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^1.20",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity": "~1.0",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/lagoon_logs": "^1.0",
        "drupal/redis": "^1.1",
        "drupal/swiftmailer": "^1.0@beta",
        "drush/drush": "^9.0.0"
    },
    "require-dev": {
        "drupal/drupal-extension": "^3.4",
        "drupal/dev-dependencies": "^8.8",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "behat/mink-selenium2-driver": "dev-master as 1.3.x-dev"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
